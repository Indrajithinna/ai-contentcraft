{
  "name": "ai-contentcraft",
  "private": true,
  "version": "1.0.0",
  "description": "A powerful multi-modal AI content generation platform using Google Gemini 2.5 Flash",
  "keywords": [
    "ai",
    "content-generation",
    "gemini",
    "react",
    "typescript"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:image": "tsx test-gemini-image.ts",
    "test:models": "tsx verify-all-models.ts",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist node_modules/.vite",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "@google/genai": "^1.30.0"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}